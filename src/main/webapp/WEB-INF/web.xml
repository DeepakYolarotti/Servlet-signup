<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
	<context-param>
	<param-name>cbranch</param-name>
	<param-value>Bangalore</param-value>
	</context-param>
	
	
	
	<servlet>
	<servlet-name>a</servlet-name>
	<servlet-class>controlers.Signup</servlet-class>
	<init-param>
	<param-name>cname</param-name>
	<param-value>Jsp</param-value>
	</init-param>
	<init-param>
	<param-name>cbranch</param-name>
	<param-value>Raj</param-value>
	</init-param>
	</servlet>
	
	<servlet>
	<servlet-name>b</servlet-name>
	<servlet-class>controlers.Login</servlet-class>
	<init-param>
	<param-name>cname</param-name>
	<param-value>Jsp</param-value>
	</init-param>
	
	</servlet>
	
	<servlet-mapping>
	<servlet-name>a</servlet-name>
	<url-pattern>/signup</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
	<servlet-name>b</servlet-name>
	<url-pattern>/login</url-pattern>
	</servlet-mapping>	
</web-app>
